#import <Foundation/Foundation.h>

@interface AlarmStateManager : NSObject<NSCoding>

//是否已经报警，为YES时，表示已经报警
@property (assign, nonatomic) BOOL hadAlarmed;

//是否已经有人接警，没有人接受，显示报警页面。
@property (assign, nonatomic) BOOL hadRecived;

@end
